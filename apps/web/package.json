{
  "name": "@infocus/web",
  "version": "1.0.0",
  "private": true,
  "description": "InFocus web application",
  "scripts": {
    "dev": "echo 'Web app dev server (to be implemented)'",
    "build": "echo 'Building web app...' && mkdir -p dist && echo '<!doctype html><html><body><div id=\"root\"></div></body></html>' > dist/index.html",
    "clean": "rm -rf dist .next",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest --config jest.config.cjs"
  },
  "dependencies": {
    "@infocus/shared": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@infocus/eslint-config": "workspace:*",
    "@infocus/jest-config": "workspace:*",
    "@infocus/prettier-config": "workspace:*",
    "@infocus/tsconfig": "workspace:*",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "typescript": "^5.3.3"
  }
}
